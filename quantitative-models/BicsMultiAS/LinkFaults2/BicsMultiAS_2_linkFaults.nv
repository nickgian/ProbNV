(* Bounded link faults *)

include "../BicsMultiAS_control.nv"

symbolic failures : int2 =
  | [0u2, 0u2] -> 0.9379899865663176p
  | [1u2, 1u2] -> 0.0600613898351553p
  | [2u2, 2u2] -> 0.0019079030788054p
  | _ -> 0.0p

symbolic f0 : tedge
symbolic f1 : tedge

let mergeLinkFaults u (x : [M]attribute) (y : [M]attribute) =
  merge u x y

let transLinkFaults d e (x : [M]attribute) =
  if (failures >u2 0u2) && ((f0 = e)|| (f1 = e)) then
    {connected=None; static=None; ospf=None; bgp=None; selected=None;}
  else trans d e x

let initLinkFaults d u = init d u 

solution rib_128_0_0_0_24 = (initLinkFaults (128.0.0.0/24), transLinkFaults (128.0.0.0/24), mergeLinkFaults)

